programa {
  funcao inicio() {
    //Algorítmo para exibir nome e idade 

    //Declaração de variaveis
    cadeia nome
    inteiro idade

    //Atribuição de variaveis
    nome = "Thifanny"
    idade = 15
    escreva("Nome:", nome, "\nIdade: ", idade)

    //Atribuição com comando Leia()
    escreva("\nDigite o seu nome: ")
